<h1>Welcome back <span class="muted"><%= @professor.nombre %></span></h1>
<div class="row">

  <div class="span4">
    <ul class="professor-ul-block">
        <li> <h4> <i class="icon-file"></i>&nbsp; Your theses</h4></li>

        <% @professor.theses.each do |t| %>
            <li><%= link_to t.title, t %></li>
        <% end %>
    </ul>
  </div>
  <div class="span4">
    <ul class="professor-ul-block">
        <li><h4> <i class="icon-user"></i>&nbsp;  Your students </h4></li>

      <% @professor.get_students.each do |s| %>
        <li><%= link_to s.name, s %></li>
      <% end %>
    </ul>
  </div>
  <div class="span4">
    <ul class="professor-ul-block">
      <li><h4> <i class="icon-calendar"> </i>&nbsp; Your schedule </h4></li>

      <% @professor.get_events.each do |s| %>
          <li><%= link_to s.title, s %></li>
      <% end %>
    </ul>

  </div>

</div>
